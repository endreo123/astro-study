---

interface Pages {
    name: string,
    href: string
};

const pages: Pages[] = [
    {
        name: 'Home',
        href: '/'
    },
    {
        name: 'About',
        href: '/about/',
    },
    {
        name: 'Blog',
        href: '/blog/',
    }
]
---
<div class="links-nav">
  {pages.map((page) => <a href={page.href}>{page.name}</a> )}
</div>
<style>
    a {
        padding: 5px;
    }
</style>
